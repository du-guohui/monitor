
page,
.container {
  background-color: #eeeeee;
}
.color1 {
  color: #0093fb;
}
image {
  width: 100%;
}
.wux-grids {
  background-color: transparent !important;
}
.buttons {
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size: 27rpx;
  border-radius: 12rpx;
  color: #ffffff;
}
.positive.buttons {
  background-color: #0093fb;
}
.calm.buttons {
  background-color: #e51c24;
}
.wux-cell__text {
  text-align: left;
  color: #333333;
  font-size: 27rpx;
  line-height: 52rpx;
  font-weight: 400;
}
.wux-search-bar__search {
  top: 24rpx !important;
}
.alarm-list .wux-card__content {
  font-size: 34rpx;
  font-weight: 400;
  color: #666666;
}
.alarm-list .text {
  font-size: 28rpx;
  color: #666666;
}
.alarm-list .wux-card__extra {
  display: block;
  font-size: 24rpx;
  flex: 0.15;
  -ms-flex: 0.15;
  width: 40rpx;
  line-height: 40rpx;
  text-align: center;
  border-radius: 8rpx;
  background: #eeeeee;
  color: #666666;
}
.alarm-list .d1 .wux-card__extra,
.alarm-list .d1 .text {
  color: #d81e06;
}
.alarm-list .d2 .wux-card__extra,
.alarm-list .d2 .text {
  color: #e6b726;
}
.alarm-list .time {
  font-size: 26rpx;
  line-height: 40rpx;
  color: #666666;
}
.group-list .wux-accordion__bd,
.group-list .wux-cell-group__bd {
  padding: 0 !important;
  background: none !important;
}
.group-list .wux-accordion__hd {
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 40rpx;
  color: #333333;
  font-size: 27rpx;
  background-color: #ffffff;
}
.group-list .wux-accordion__arrow {
  top: 30rpx;
  right: 30rpx;
  width: 20rpx;
  height: 20rpx;
}
.wux-cell-group__hd {
  margin-bottom: 6rpx;
}
.group-box .wux-input,
.group-box .wux-input__label {
  line-height: 50rpx;
}
.group-box .wux-cell {
  padding: 10rpx 40rpx;
}
.group-box .wux-input__label {
  height: 60rpx;
  margin-top: 16rpx;
}
.group-list .checkbox .wux-cell:after {
  display: none;
}
.wux-button {
  font-size: 24rpx !important;
}
.wux-button--positive.wux-button--hover {
  background-color: #0093fb !important;
  color: #fff !important;
}
.wux-slider__handle {
  width: 40rpx !important;
  height: 40rpx !important;
}
.groups .wux-cell-group__bd {
  background: none;
}
.is_default .wux-cell {
  margin-top: 30rpx !important;
}
.groups .wux-cell {
  background: #ffffff !important;
}
.container .wux-button--positive.wux-button--hover,
.container .wux-button--positive {
  background-color: #0093fb !important;
  color: #fff !important;
}
.wux-select__button--confirm,
.wux-dialog__button--primary {
  color: #0093fb !important;
}
.prompt {
  position: fixed;
  width: 100%;
  height: 100%;
  overflow: hidden;
  z-index: 99;
}
.prompt .box {
  width: 80%;
}
.prompt .box,
.prompt .iocs {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}
.prompt .ioc {
  background: #0093fb;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
  margin: 20rpx auto;
  position: relative;
  overflow: hidden;
}
.prompt .title {
  line-height: 52rpx;
  font-size: 28rpx;
  text-align: center;
  font-weight: 300;
}
.GroupName .wux-cell {
  padding: 4rpx 10rpx;
}
.GroupName .wux-input__label {
  line-height: 80rpx;
  font-size: 26rpx;
}
.GroupName .wux-input__item {
  text-align: left;
}
.wux-filterbar__item {
  font-size: 26rpx !important;
}
.filterbar .wux-filterbar__select {
  top: 84rpx !important;
}
.checkbox .wux-cell {
  padding: 42rpx 30rpx;
  padding-left: 660rpx;
}
.checkbox .wux-cell .wux-selectable {
  float: right;
}
.list-button {
  box-shadow: 0 0 6rpx rgba(0, 0, 0, 0.3);
  position: fixed;
  bottom: 0;
  height: 128rpx;
  background: #ffffff;
  left: 0;
  right: 0;
  z-index: 101;
}
.list-button .button {
  width: 332rpx;
  float: left;
  height: 140rpx;
  padding: 0 20rpx;
}
.list-button.button2 .button {
  width: auto;
  float: none;
  height: 140rpx;
  padding: 0 20rpx;
}
.detail-ts .wux-cell__ft {
  font-size: 26rpx;
  color: red;
  font-weight: bold;
}